{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "lib": [
      "ES2022"
    ],
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "moduleResolution": "node",
    "allowJs": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmit": true
  }
}
